{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "API Requirement",
    "license": {
      "name": "MIT"
    }
  },
  "host": "usk.ac.id",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/get-data-h2h": {
      "get": {
        "summary": "Get Data H2H",
        "operationId": "getDataH2h",
        "tags": [
          "h2h"
        ],
        "parameters": [
          {
            "name": "mulaitanggal",
            "in": "query",
            "description": "Tanggal setelah data h2h",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "sampaitanggal",
            "in": "query",
            "description": "Tanggal sebelum data h2h",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "tahun",
            "in": "query",
            "description": "Tahun data h2h",
            "required": false,
            "type": "integer",
            "format": "year"
          },
          {
            "name": "semester",
            "in": "query",
            "description": "Semester data h2h",
            "required": false,
            "type": "string",
          },
          {
            "name": "jenjang",
            "in": "query",
            "description": "Jenjang mahasiswa data h2h",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "An paged array data h2h",
            "headers": {
            },
            "schema": {
              "$ref": "#/definitions/DataH2HArray"
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "DataH2H": {
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "tahun": {
          "type": "integer",
          "format": "year"
        },
        "semester": {
          "type": "string"
        },
        "nim": {
          "type": "string"
        },
        "jenjang": {
          "type": "string"
        },
        "nominal_spp_ukt": {
          "type": "number"
        },
        "nominal_spi_bop": {
          "type": "number"
        },
        "nominal_denda": {
          "type": "number"
        },
        "nominal_matrikulasi": {
          "type": "number"
        },
        "id_bank_penerima": {
          "type": "string"
        }
      }
    },
    "DataH2HArray": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/DataH2H"
      }
    },
    "Error": {
      "required": [
        "code",
        "message"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}